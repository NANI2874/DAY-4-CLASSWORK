#include <stdio.h>

struct student {
  char name[50];
  int id;
  float gpa;
};

int main() {
  // Create a new student instance
  struct student s;

  // Assign values to the fields
  strcpy(s.name, "John");
  s.id = 12345;
  s.gpa = 3.8;

  // Print out the values
  printf("Name: %s\n", s.name);
  printf("ID: %d\n", s.id);
  printf("GPA: %f\n", s.gpa);

  return 0;
}
